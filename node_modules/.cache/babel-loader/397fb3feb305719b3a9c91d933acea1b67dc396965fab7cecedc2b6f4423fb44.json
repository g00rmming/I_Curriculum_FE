\n      this.$swal({\n        title: \"회원가입 하시겠습니까?\",\n        type: \"warning\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonClass: \"btn-danger\",\n        confirmButtonText: \"예\",\n        cancelButtonText: \"아니오\",\n        closeOnConfirm: true,\n        closeOnCancel: true\n      }).then((result) => {\n        if (result.isConfirmed) {\n          this.$axios.post(`${this.apiurl}/join`, {\n            clientId: this.menberInfo.clientId,\n            password: this.menberInfo.password,\n            nickname: this.menberInfo.nickname,\n            department_name: this.menberInfo.department_name,\n            joinYear: 2019,\n            compeleteTerm: 4\n          }).then((res) => {\n            console.log(res);\n            if (res) {\n              this.$swal('완료 되었습니다.', '', 'success')\n              router.push('/signin')\n            } else {\n              alert('실패')\n            }\n          }).catch((err) => {\n            console.log(err);\n          })\n        }\n      })\n    }\n  }\n}\n</script>"],"mappings":";AAqEA,OAAOA,MAAK,MAAO,UAAS;AAM5B,eAAe;EACbC,MAAM,EAAE,CAAC,QAAQ,CAAC;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,MAAM,EAAE,aAAa;MACrBC,UAAU,EAAE;QAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE,EAAE;QACnBC,QAAQ,EAAE,CAAC;QACXC,aAAa,EAAE;MACjB,CAAC;MACDC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACF,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAiB;IAClD,CAAC;IACDG,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,KAAK,CAAC;QACTC,KAAK,EAAE,cAAc;QACrBC,IAAI,EAAE,SAAS;QACfC,IAAI,EAAE,UAAU;QAChBC,gBAAgB,EAAE,IAAI;QACtBC,kBAAkB,EAAE,YAAY;QAChCC,iBAAiB,EAAE,GAAG;QACtBC,gBAAgB,EAAE,KAAK;QACvBC,cAAc,EAAE,IAAI;QACpBC,aAAa,EAAE;MACjB,CAAC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAK;QAClB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACtB,IAAI,CAACC,MAAM,CAACC,IAAI,CAAC,GAAG,IAAI,CAAC1B,MAAM,OAAO,EAAE;YACtCE,QAAQ,EAAE,IAAI,CAACD,UAAU,CAACC,QAAQ;YAClCC,QAAQ,EAAE,IAAI,CAACF,UAAU,CAACE,QAAQ;